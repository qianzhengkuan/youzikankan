<view class="flex-box flex-column"><view class="sh-bg" style="{{(chooseShImage)}}"><view class="sh-bg-con  flex-box flex-xy-center flex-column"><text>{{chooseShName?chooseShName:'请选择你要加入的商会'}}</text><button data-event-opts="{{[['tap',[['navTo',['selectSH']]]]]}}" class="yzkk-submitBtn" bindtap="__e">选择商会</button></view><view class="black-bg"></view></view><view class="req-title bd-bottom"><text>入会申请</text></view><view class="form-panel flex-1"><block wx:for="{{formList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="form-list bd-bottom"><view class="form-item flex-box flex-y-center" hover-class="{{item.type=='select'?'yzkk-href-hover':''}}"><view class="label"><text>{{item.label}}</text></view><view class="flex-1"><block wx:if="{{item.type=='number'}}"><input name="{{item.name}}" maxlength="11" placeholder="{{'请输入'+item.label}}" data-event-opts="{{[['input',[['__set_model',['$0','val','$event',[]],[[['formList','',index]]]]]]]}}" value="{{item.val}}" bindinput="__e"/></block><block wx:if="{{item.type=='text'}}"><input name="{{item.name}}" placeholder="{{'请输入'+item.label}}" data-event-opts="{{[['input',[['__set_model',['$0','val','$event',[]],[[['formList','',index]]]]]]]}}" value="{{item.val}}" bindinput="__e"/></block><block wx:if="{{item.type=='select1'}}"><view data-event-opts="{{[['tap',[['showPicker',['selector1']]]]]}}" class="jiantouRight lineJianTou" bindtap="__e"><text class="{{[(index1)?'black-text':'']}}">{{array1[index1].name?array1[index1].name:'请选择'}}</text></view></block><block wx:if="{{item.type=='select2'}}"><view data-event-opts="{{[['tap',[['showPicker',['selector2']]]]]}}" class="jiantouRight lineJianTou" bindtap="__e"><text class="{{[(index2)?'black-text':'']}}">{{array2[index2].name?array2[index2].name:'请选择'}}</text></view></block><block wx:if="{{item.type=='select3'}}"><view data-event-opts="{{[['tap',[['showPicker',['region']]]]]}}" class="jiantouRight lineJianTou" bindtap="__e"><text class="{{[(addressRengin)?'black-text':'']}}">{{addressRengin?addressRengin:'请选择'}}</text></view></block></view></view></view></block><view class="flex-box flex-y-center addressInfo"><textarea name="addressInfo" maxlength="20" placeholder="请输入详细地址" data-event-opts="{{[['input',[['__set_model',['','addressInfo','$event',[]]]]]]}}" value="{{addressInfo}}" bindinput="__e"></textarea></view></view><w-picker class="vue-ref" vue-id="d89f5554-1" mode="selector" value="{{array1[index1].name}}" default-type="name" default-props="{{defaultProps}}" options="{{array1}}" data-ref="selector1" data-event-opts="{{[['^confirm',[['onConfirm',['$event','selector1']]]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker><w-picker class="vue-ref" vue-id="d89f5554-2" mode="selector" value="{{array2[index2].name}}" default-type="name" default-props="{{defaultProps}}" options="{{array2}}" data-ref="selector2" data-event-opts="{{[['^confirm',[['onConfirm',['$event','selector2']]]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker><w-picker class="vue-ref" vue-id="d89f5554-3" mode="region" value="{{defaultRegion}}" default-type="value" hide-area="{{false}}" data-ref="region" data-event-opts="{{[['^confirm',[['onConfirm',['$event','region']]]]]}}" bind:confirm="__e" bind:__l="__l"></w-picker><view class="yzkk-submitBtn joinNow" hover-class="yzkk-btn-hover" data-event-opts="{{[['tap',[['joinNow',['$event']]]]]}}" bindtap="__e">提交申请</view></view>