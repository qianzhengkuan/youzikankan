<view class="activity-list"><view class="activity-item bd-bottom flex-box flex-column" hover-class="yzkk-href-hover" data-event-opts="{{[['tap',[['navTo',['/pages/activity/activityInfo','$0'],['shData']]]]]}}" bindtap="__e"><image class="cover" src="{{shData.image}}" mode="aspectFill" data-event-opts="{{[['error',[['showErrorImage',['$event']]]]]}}" binderror="__e"></image><view class="title text-over text-over-2">{{shData.name}}</view><view class="flex-box"><view class="bottom flex-box flex-y-center"><image class="address" src="../../static/images/icon/dignwei-02.png" mode></image><text class="text-over">{{''+shData.address+''}}</text></view></view><view class="{{['state-icon',(shData.type===0)?'noBegin':'',(shData.type===1)?'beging':'',(shData.type===-1)?'over':'']}}">{{shData.type_name}}</view></view></view>