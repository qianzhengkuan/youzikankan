<view class="container"><view class="top-banner-con bd-bottom"><view class="swiper-margin-wrap"><swiper indicator-dots="{{true}}" indicator-active-color="#ffffff" autoplay="{{true}}" interval="{{3000}}" duration="{{1000}}"><block wx:for="{{shData.images}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item class="swiper-item"><view><image src="{{item.url}}" mode="aspectFill" data-event-opts="{{[['error',[['showErrorImage',[0,index]]]]]}}" binderror="__e"></image></view></swiper-item></block></swiper></view><view class="tongxiang  flex-box flex-y-center"><image src="{{shData.img}}" mode="aspectFill" data-event-opts="{{[['error',[['showErrorImage',[1]]]]]}}" binderror="__e"></image><view class="flex-1 flex-box flex-column right-con _div"><view class="name">{{shData.name}}</view><text class="gray-text">{{peopleList.length+"位成员/"+shData.create_time+"建会"}}</text><text class="gray-text address">{{shData.address}}</text></view></view></view><view class="cover-wrap"><panel-top vue-id="7d29b6eb-1" title="商会成员" url="{{peopleList.length?'peopleList?id='+shData.id+'':''}}" bind:__l="__l"></panel-top><view class="flex-box people-list"><block wx:for="{{peopleList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<4}}"><view class="p-item"><image src="{{item.image}}" mode="aspectFill" data-event-opts="{{[['error',[['showErrorImage',[2,index]]]]]}}" binderror="__e"></image><view class="name text-over">{{item.username}}</view><view class="gray">{{item.grade}}</view></view></block></block><block wx:if="{{!peopleList.length}}"><no-more vue-id="7d29b6eb-2" bind:__l="__l"></no-more></block></view></view><view class="{{['cover-wrap',(!isMySh)?'noMySh':'']}}"><panel-top vue-id="7d29b6eb-3" title="商会资讯" url="newsList" bind:__l="__l"></panel-top><block wx:for="{{newsData}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view><block wx:if="{{item.type==1}}"><text-new vue-id="{{'7d29b6eb-4-'+index}}" newsData="{{item}}" bind:__l="__l"></text-new></block><block wx:if="{{item.type==2}}"><new-list vue-id="{{'7d29b6eb-5-'+index}}" newsData="{{item}}" textOver2="{{true}}" isReverse="{{true}}" bind:__l="__l"></new-list></block><block wx:if="{{item.type==3}}"><big-cover vue-id="{{'7d29b6eb-6-'+index}}" coverData="{{item}}" bind:__l="__l"></big-cover></block><block wx:if="{{item.type==4}}"><three-new vue-id="{{'7d29b6eb-7-'+index}}" threeData="{{item}}" bind:__l="__l"></three-new></block></view></block><block wx:if="{{!newsData.length}}"><no-more vue-id="7d29b6eb-8" bind:__l="__l"></no-more></block></view><block wx:if="{{bottomShow&&!isMySh}}"><view class="bottom-opt flex-y-center fixed-bottom"><block wx:if="{{!isSelect&&!isMySh}}"><view class="yzkk-submitBtn join-btn" hover-class="yzkk-btn-hover" data-event-opts="{{[['tap',[['navTo',['shanghuiJoin?id='+shData.id+'&name='+shData.name+'']]]]]}}" bindtap="__e">立即加入</view></block><block wx:if="{{isSelect}}"><view class="yzkk-submitBtn join-btn" hover-class="yzkk-btn-hover" data-event-opts="{{[['tap',[['backToForm',['$event']]]]]}}" bindtap="__e">申请加入</view></block></view></block></view>